{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "hexo clean && hexo generate && echo {\"version\": \"$(git rev-parse --short HEAD)\"} > public/version.json",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server",
    "start": "hexo clean && hexo generate && echo {\"version\": \"$(git rev-parse --short HEAD)\"} > public/version.json && hexo server"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "hexo": "^7.3.0",
    "hexo-abbrlink": "^2.2.1",
    "hexo-deployer-git": "^4.0.0",
    "hexo-excerpt": "^1.3.1",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-log": "^4.1.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-inferno": "^1.0.2",
    "hexo-renderer-marked": "^7.0.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-icarus": "^6.1.1",
    "hexo-theme-landscape": "^1.0.0"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "hexo-util",
      "inferno"
    ]
  }
}
